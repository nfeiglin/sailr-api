{
    "name": "sailr/vip",
    "description": "A package built on top of Laravel Cashier for allowing / denying access to parts of the application based on the user's Stripe subscription",
    "authors": [
        {
            "name": "Nathan Feiglin",
            "email": "nathanfeiglin@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "4.2.*",
        "nesbot/Carbon": "~1.9",
        "laravel/cashier": "~2.0"

    },
    "autoload": {
        "classmap": [
            "src/migrations"
        ],
        "psr-0": {
            "Sailr\\Vip\\": "src/"
        }
    },
    "minimum-stability": "stable"
}
